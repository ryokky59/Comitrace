<tr class="nested-fields">
  <td><%= f.text_field :plan, maxlength: 10, size: 15 %></td>
  <td>:</td>
  <td><%= f.time_field :start_time, step: 600, id: "#{@schedule.schedule_plans}" %></td>
  <td>〜</td>
  <td><%= f.time_field :end_time, step: 600 %></td>
  <td><%= link_to_remove_association "削除", f, class: "btn btn-sm btn-danger" %></td>
</tr>
